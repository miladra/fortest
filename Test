import com.datastax.driver.mapping.annotations.*;

@Entity
@Table(name = "t_accounttypeitem")
public class AccountTypeItem {
    
    @PartitionKey
    private BigDecimal id;
    
    @Column(name = "c_acccredcirccn")
    private BigDecimal accCreditCircCN;
    
    @Column(name = "c_accdebtcirccn")
    private BigDecimal accDebtCircCN;
    
    @Column(name = "c_accnextam")
    private BigDecimal accNextAm;
    
    @Column(name = "c_accountid")
    private BigDecimal accountId;
    
    @Column(name = "c_accounttypeid")
    private BigDecimal accountTypeId;
    
    @Column(name = "c_accprevam")
    private BigDecimal accPrevAm;
    
    @Column(name = "c_accttcredam")
    private BigDecimal accTtCredAm;
    
    @Column(name = "c_accttdebtam")
    private BigDecimal accTtDebtAm;
    
    @Column(name = "c_active")
    private BigDecimal active;
    
    @Column(name = "c_amount")
    private BigDecimal amount;
    
    @Column(name = "c_blockingitem")
    private BigDecimal blockingItem;
    
    @Column(name = "c_branchid")
    private BigDecimal branchId;
    
    @Column(name = "c_comment")
    private String comment;
    
    @Column(name = "c_creditorcirculationcount")
    private BigDecimal creditorCirculationCount;
    
    @Column(name = "c_currencyid")
    private BigDecimal currencyId;
    
    @Column(name = "c_debtor")
    private BigDecimal debtor;
    
    @Column(name = "c_debtorcirculationcount")
    private BigDecimal debtorCirculationCount;
    
    @Column(name = "c_document")
    private BigDecimal document;
    
    @Column(name = "c_documentid")
    private BigDecimal documentId;
    
    @Column(name = "c_documentitem")
    private BigDecimal documentItem;
    
    @Column(name = "c_documentitemid")
    private BigDecimal documentItemId;
    
    @Column(name = "c_enable")
    private BigDecimal enable;
    
    @Column(name = "c_eqam")
    private BigDecimal eqAm;
    
    @Column(name = "c_eqatiid")
    private BigDecimal eqAtiId;
    
    @Column(name = "c_eqcredcircn")
    private BigDecimal eqCredCircN;
    
    @Column(name = "c_eqdebtcircn")
    private BigDecimal eqDebtCircN;
    
    @Column(name = "c_eqdocitemid")
    private BigDecimal eqDocItemId;
    
    @Column(name = "c_eqttam")
    private BigDecimal eqTtAm;
    
    @Column(name = "c_eqttcredam")
    private BigDecimal eqTtCredAm;
    
    @Column(name = "c_eqttdebtam")
    private BigDecimal eqTtDebtAm;
    
    @Column(name = "c_indicator")
    private BigDecimal indicator;
    
    @Column(name = "c_issuanceid")
    private String issuanceId;
    
    @Column(name = "c_lateitem")
    private BigDecimal lateItem;
    
    @Column(name = "c_manualid")
    private String manualId;
    
    @Column(name = "c_oppositedocumentitem")
    private BigDecimal oppositeDocumentItem;
    
    @Column(name = "c_relatedbranch")
    private BigDecimal relatedBranch;
    
    @Column(name = "c_row")
    private BigDecimal row;
    
    @Column(name = "c_totalamount")
    private BigDecimal totalAmount;
    
    @Column(name = "c_totalcreditoramount")
    private BigDecimal totalCreditorAmount;
    
    @Column(name = "c_totaldebtoramount")
    private BigDecimal totalDebtorAmount;
    
    @Column(name = "c_version")
    private BigDecimal version;
    
    @SecondaryIndex
    @Column(name = "i_document")
    private BigDecimal iDocument;
    
    // constructors, getters and setters
    
    public AccountTypeItem() {}
    
    public AccountTypeItem(BigDecimal id, BigDecimal accCreditCircCN, BigDecimal accDebtCircCN, 
                           BigDecimal accNextAm, BigDecimal accountId, BigDecimal accountTypeId,
                           BigDecimal accPrevAm, BigDecimal accTtCredAm, BigDecimal accTtDebtAm,
                           BigDecimal active, BigDecimal amount, BigDecimal blockingItem, 
                           BigDecimal branchId, String comment, BigDecimal creditorCirculationCount, 
                           BigDecimal currencyId, BigDecimal debtor, BigDecimal debtorCirculationCount,
                           BigDecimal document, BigDecimal documentId, BigDecimal documentItem,
                           BigDecimal documentItemId, BigDecimal enable, BigDecimal eqAm,

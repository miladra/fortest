import javax.persistence.*;

@Entity
@Table(name = "customer_picture")
public class CustomerPicture {

    @Id
    @Column(name = "customer_number", nullable = false)
    private String customerNumber;

    @Column(name = "branch_code")
    private String branchCode;

    @Column(name = "branch_id")
    private Long branchId;

    @Column(name = "customer_id")
    private Long customerId;

    @Column(name = "etl_date")
    private String etlDate;

    @Column(name = "idendocs_id")
    private Long idendocsId;

    @Column(name = "idendocs_imageid")
    private Long idendocsImageid;

    @Column(name = "idendocs_type_id")
    private String idendocsTypeId;

    @Column(name = "idendocs_type_title")
    private String idendocsTypeTitle;

    @Column(name = "image_indry", columnDefinition = "BLOB")
    private byte[] imageIndry;
    
    // constructors, getters and setters
}

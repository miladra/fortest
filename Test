@Entity
@Table(name = "deposit")
public class Deposit {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long id;

    @Column(name = "main_customer_number")
    private String mainCustomerNumber;

    @Column(name = "deposit_number")
    private String depositNumber;

    @Column(name = "branch_code")
    private String branchCode;

    @Column(name = "branch_id")
    private Long branchId;

    @Column(name = "currency_id")
    private Long currencyId;

    @Column(name = "deposit_status_id")
    private Long depositStatusId;

    @Column(name = "deposit_status_name")
    private String depositStatusName;

    @Column(name = "deposit_type_name")
    private String depositTypeName;

    @Column(name = "etl_date")
    private String etlDate;

    @Column(name = "expire_date")
    private String expireDate;

    @Column(name = "min_amount")
    private String minAmount;

    // constructors, getters and setters, toString, etc.
    
}

import javax.persistence.*;

@Entity
@Table(name = "user_authority")
public class UserAuthority {

    @EmbeddedId
    private UserAuthorityId id;

    @Column(name = "authority_expiration_date_from")
    private String authorityExpirationDateFrom;

    @Column(name = "authority_expiration_date_to")
    private String authorityExpirationDateTo;

    @Column(name = "authority_id")
    private Long authorityId;

    @Column(name = "authority_is_active")
    private Integer authorityIsActive;

    @Column(name = "branch_id")
    private Long branchId;

    @Column(name = "etl_date")
    private String etlDate;

    @Column(name = "first_name")
    private String firstName;

    @Column(name = "full_name")
    private String fullName;

    @Column(name = "group_name")
    private String groupName;

    @Column(name = "group_number")
    private String groupNumber;

    @Column(name = "has_box_access_permission")
    private Integer hasBoxAccessPermission;

    @Column(name = "last_active_box_account_id")
    private Long lastActiveBoxAccountId;

    @Column(name = "last_name")
    private String lastName;

    @Column(name = "password")
    private String password;

    @Column(name = "responsibility_id")
    private Long responsibilityId;

    @Column(name = "responsibility_title")
    private String responsibilityTitle;

    @Column(name = "user_id")
    private String userId;

    // getters and setters

    public UserAuthorityId getId() {
        return id;
    }

    public void setId(UserAuthorityId id) {
        this.id = id;
    }

    public String getAuthorityExpirationDateFrom() {
        return authorityExpirationDateFrom;
    }

    public void setAuthorityExpirationDateFrom(String authorityExpirationDateFrom) {
        this.authorityExpirationDateFrom = authorityExpirationDateFrom;
    }

    public String getAuthorityExpirationDateTo() {
        return authorityExpirationDateTo;
    }

    public void setAuthorityExpirationDateTo(String authorityExpirationDateTo) {
        this.authorityExpirationDateTo = authorityExpirationDateTo;
    }

    public Long getAuthorityId() {
        return authorityId;
    }

    public void setAuthorityId(Long authorityId) {
        this.authorityId = authorityId;
    }

    public Integer getAuthorityIsActive() {
        return authorityIsActive;
    }

    public void setAuthorityIsActive(Integer authorityIsActive) {
        this.authorityIsActive = authorityIsActive;
    }

    public Long getBranchId() {
        return branchId;
    }

    public void setBranchId(Long branchId) {
        this.branchId = branchId;
    }

    public String getEtlDate() {
        return etlDate;
    }

    public void setEtlDate(String etlDate) {
        this.etlDate = etlDate;
    }

    public String getFirstName() {
        return firstName;
    }

    public void setFirstName(String firstName) {
        this.firstName = firstName;
    }

    public String getFullName() {
        return fullName;
    }

    public void setFullName(String fullName) {
        this.fullName = fullName;
    }

    public String getGroupName() {
        return groupName;
    }

    public void setGroupName(String groupName) {
        this.groupName = groupName;
    }

    public String getGroupNumber() {
        return groupNumber;
    }

    public void setGroupNumber(String groupNumber) {
        this.groupNumber = groupNumber;
    }

    public Integer getHasBoxAccessPermission() {
        return hasBoxAccessPermission;
    }

    public void setHasBoxAccessPermission(Integer hasBoxAccessPermission) {
        this.hasBoxAccessPermission = hasBoxAccessPermission;
    }

    public Long getLastActiveBoxAccountId() {
        return lastActiveBoxAccountId;
    }

    public void setLastActiveBoxAccountId(Long lastActiveBoxAccountId) {
        this.lastActiveBoxAccountId = lastActiveBoxAccountId;
    }

    public String getLastName() {
        return lastName;
    }

    public void setLastName(String lastName) {
        this.lastName = lastName;
    }

    public String getPassword() {
        return password;
    }

    public void setPassword(String password) {
        this.password = password;
    }

    public Long getResponsibilityId() {
        return responsibilityId;
    }

    public void setResponsibilityId(Long responsibilityId) {
        this.responsibilityId = responsibilityId;
    }

    public String getResponsibilityTitle() {
        return responsibilityTitle;
    }

    public void setResponsibilityTitle(String responsibilityTitle) {
        this.responsibilityTitle = responsibilityTitle;
    }

    public String getUserId() {
        return userId;
    }

    public void setUserId(String userId) {
        this.userId = userId;
